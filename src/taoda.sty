%\myvec usage:
%\myvec{0}[1][2][][3][4] -> C_1,C_2,\cdots,C_3,C_4
%\myvec[\to]{0}[1][2][][3][4] -> C_1\to C_2\to \cdots\to C_3\to C_4
\def\TYF@myvecA[#1]{%
    \def\TYF@connective{#1}
    \TYF@myvecB
}%
\def\TYF@myvecB#1{%
    \def\TYF@mainpart{#1}%
    \TYF@myvecC [\@empty]%
}%
\def\TYF@myvecC[#1][#2]{%
    \def\TYF@temp{#2}%
    \ifx\TYF@temp\@empty%
        {#1}\cdots%
    \else%
        {#1}\TYF@mainpart_{#2}%
    \fi%
    \@ifnextchar [{%
        \TYF@myvecC[\TYF@connective]%
    }{%
    }%
}
\def\myvec{
    \@ifnextchar [{
        \TYF@myvecA
    }{
        \TYF@myvecA[,\,]
    }
}
